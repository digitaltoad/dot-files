	"publisher": "forbeslindesay",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ForbesLindesay/vscode-sql-template-literal.git"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
