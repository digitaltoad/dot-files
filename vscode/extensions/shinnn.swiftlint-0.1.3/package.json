{
	"name": "swiftlint",
	"version": "0.1.3",
	"displayName": "SwiftLint",
	"description": "Use SwiftLint to lint Swift files",
	"publisher": "shinnn",
	"repository": "https://github.com/shinnn/vscode-swiftlint",
	"homepage": "https://github.com/shinnn/vscode-swiftlint#readme",
	"bugs": "https://github.com/shinnn/vscode-swiftlint/issues",
	"license": "MIT",
	"icon": "media/icon.svg",
	"galleryBanner": {
		"color": "#F2F2F2",
		"theme": "light"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Linters"
	],
	"activationEvents": [
		"onLanguage:swift"
	],
	"main": "extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "SwiftLint configuration options",
			"properties": {
				"swiftlint.enable": {
					"type": "boolean",
					"default": false,
					"description": "Control whether SwiftLint is enabled for Swift files or not."
				}
			}
		}
	},
	"scripts": {
		"test": "eslint --config @shinnn/node --fix extension.js parser.js server.js"
	},
	"dependencies": {
		"concat-stream": "1.5.1",
		"vscode-languageclient": "0.10.7",
		"vscode-languageserver": "0.10.6",
		"which-promise": "1.0.0"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^1.0.1",
		"eslint": "^1.10.0",
		"vscode": "^0.10.4"
	},
	"__metadata": {
		"id": "fe330a3b-84aa-447a-9312-99734f91d279",
		"publisherId": "253db61f-997d-4722-8120-7559a25856e6",
		"publisherDisplayName": "Shinnosuke Watanabe"
	}
}